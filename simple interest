#include <stdio.h>
int main ()
{
    float P,R,T,SI;
    printf("Enter principal amount :")
    scanf("%f",&P)
    printf("Enter rate of interest percentage :")
    scanf("%f",&R)
    printf("Enter time period in years :")
    scanf("%f",&T)
  
    printf("Simple Interest is %f " , P*R*T/100);
    return 0;
}
