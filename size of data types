#include<stdio.h>
int main() {
    int int-DataType;
    float float-DataType;
    double double-DataType;
    char char-DataType;

    
    printf("Size of int: %u bytes", sizeof(int-DataType));
    printf("Size of float: %u bytes", sizeof(float-DataType));
    printf("Size of double: %u bytes", sizeof(double-DataType));
    printf("Size of char: %u byte", sizeof(char-DataType));
    
    return 0;
}
